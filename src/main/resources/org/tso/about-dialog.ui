<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <object class="GtkWindow" id="infoDialog">
    <property name="default-width">400</property>
    <property name="default-height">350</property>
    <property name="visible">false</property>
    <property name="resizable">false</property>
    <property name="modal">true</property>
    <property name="hide_on_close">true</property>
    <property name="title" translatable="1">Hex Viewer - About</property>
    <child>
      <object class="GtkGrid">
        <child>
          <object class="GtkBox" id="vbox1">
            <layout>
              <property name="column">0</property>
              <property name="row">0</property>
              <property name="column-span">1</property>
            </layout>
            <property name="orientation">GTK_ORIENTATION_HORIZONTAL</property>
            <property name="hexpand">1</property>
            <property name="vexpand">1</property>
            <child>
                  <object class="GtkScrolledWindow" id="scrolledWindowAttribute">
                    <property name="visible">true</property>
                    <property name="has_frame">true</property>
                    <property name="vexpand">0</property>
                    <property name="hexpand">1</property>
                    <property name="margin_start">14</property>
                     <property name="margin_end">14</property>
                    <property name="margin_top">10</property>
                    <property name="min_content_height">200</property>
                    <property name="max_content_height">200</property>
                    <child>
                      <object class="GtkTextView" id="infoViewer">
                        <property name="editable">false</property>
                        <property name="left_margin">10</property>
                        <property name="right_margin">10</property>
                        <property name="bottom_margin">10</property>
                        <property name="top_margin">10</property>
                      </object>
                    </child>
                  </object>
                </child>
          </object>
        </child>
        <child>
          <object class="GtkBox" id="vbox2">
            <property name="hexpand">1</property>
            <layout>
              <property name="column">0</property>
              <property name="row">1</property>
               <property name="column-span">1</property>
            </layout>
            <property name="orientation">GTK_ORIENTATION_HORIZONTAL</property>
            <property name="hexpand">1</property>
            <child type="action">
              <object class="GtkButton" id="button_ok">
                <property name="margin_top">10</property>
                <property name="margin_bottom">10</property>
                <property name="margin_start">350</property>
                <property name="margin_end">14</property>            
                <property name="icon-name">window-close-symbolic</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>